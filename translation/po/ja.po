#. type: Labeled list
#: ../share/man/man1/ruby-build.1.adoc:113
#, no-wrap
msgid "*RUBY_BUILD_MIRROR_PACKAGE_URL*"
msgstr "*RUBY_BUILD_MIRROR_PACKAGE_URL*"

#. type: Plain text
#: ../share/man/man1/ruby-build.1.adoc:115
msgid "Custom complete mirror URL"
msgstr "独自の完全なミラーURLです"

#. type: Labeled list
#: ../share/man/man1/ruby-build.1.adoc:116
#, no-wrap
msgid "*RUBY_BUILD_SKIP_MIRROR*"
