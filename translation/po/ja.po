#. type: Labeled list
#: ../share/man/man1/ruby-build.1.adoc:125
#, no-wrap
msgid "*RUBY_BUILD_DEFINITIONS*"
msgstr "*RUBY_BUILD_DEFINITIONS*"

#. type: Plain text
#: ../share/man/man1/ruby-build.1.adoc:127
msgid "Paths to search for build definitions in addition to _RUBY_BUILD_ROOT_"
msgstr "_RUBY_BUILD_ROOT_ に加えて、ビルド定義を探索するパスです"

#. type: Labeled list
#: ../share/man/man1/ruby-build.1.adoc:128
#, no-wrap
msgid "*CC*"
msgstr "*CC*"

#. type: Plain text
#: ../share/man/man1/ruby-build.1.adoc:130
msgid "Path to the C compiler"
msgstr "Cコンパイラへのパスです"

#. type: Labeled list
#: ../share/man/man1/ruby-build.1.adoc:131
#, no-wrap
msgid "*RUBY_CFLAGS*"
msgstr "*RUBY_CFLAGS*"

#. type: Plain text
